{
    "name": "borda",
    "version": "0.0.0-dev",
    "engines": {
        "node": ">=16",
        "npm": ">=8"
    },
    "private": true,
    "scripts": {
        "start": "remix-serve build",
        "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
        "dev:remix": "remix dev",
        "dev:css": "tailwindcss -w -i ./styles/tailwind.css -o app/styles/tailwind.css",
        "build": "npm run build:css && remix build",
        "build:css": "tailwindcss -m -i ./styles/tailwind.css -o app/styles/tailwind.css"
    },
    "prisma": {
        "seed": "node prisma/seed.js"
    },
    "dependencies": {
        "@headlessui/react": "^1.6.6",
        "@heroicons/react": "^2.0.12",
        "@mapbox/node-pre-gyp": "^1.0.10",
        "@prisma/client": "^4.3.1",
        "@remix-run/node": "^1.6.8",
        "@remix-run/react": "^1.6.8",
        "@remix-run/serve": "^1.6.8",
        "aws-sdk": "^2.1230.0",
        "bcrypt": "^5.0.1",
        "clsx": "^1.2.1",
        "dayjs": "^1.11.0",
        "error-stack-parser": "^2.1.4",
        "framer-motion": "7.6.7",
        "js-yaml": "^4.1.0",
        "mock-aws-s3": "^4.0.2",
        "moment": "^2.29.4",
        "moment-timezone": "^0.5.39",
        "nock": "^13.2.9",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-markdown": "^6.0.0",
        "react-type-animation": "^2.1.2",
        "react-typist": "^2.0.5",
        "remark-gfm": "^3.0.1",
        "remix-auth": "^3.2.2",
        "remix-auth-form": "^1.1.2",
        "zod": "^3.19.1"
    },
    "devDependencies": {
        "@remix-run/dev": "^1.3.4",
        "@remix-run/eslint-config": "^1.3.4",
        "@shrutibalasa/tailwind-grid-auto-fit": "^1.1.0",
        "autoprefixer": "^10.4.8",
        "concurrently": "^7.3.0",
        "eslint": "^8.11.0",
        "postcss": "^8.4.16",
        "prisma": "^4.3.1",
        "rmx-cli": "^0.3.1",
        "tailwindcss": "^3.1.8",
        "tailwindcss-inner-border": "^0.2.0"
    }
}
